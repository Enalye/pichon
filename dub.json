    "dependencies": {
        "atelier": {
            "path": "atelier"
        }
    },
    "description": "Simple UI tool for Real-ESRGAN",
    "license": "ZLib",
    "name": "pichon",
    "targetName": "pichon",
    "targetType": "executable",
    "workingDirectory": "",
    "configurations": [
        {
            "name": "debug",
            "platforms": [
                "windows"
            ],
            "targetPath": "bin"
        },
        {
            "name": "windows-build",
            "platforms": [
                "windows"
            ],
            "targetPath": "bin"
        },
        {
            "name": "windows-release",
            "platforms": [
                "windows"
            ],
            "versions": [
                "ReleaseApp"
            ],
            "lflags": [
                "-subsystem:windows",
                "-entry:mainCRTStartup"
            ],
            "targetPath": "export",
            "preBuildCommands": [
                "if exist export\\ rmdir export\\ /q /s"
            ],
            "postBuildCommands": [
                "copy libs\\*.dll export\\",
                "copy icon.png export\\icon.png",
                "xcopy /E img\\ export\\img\\",
                "xcopy /E locale\\ export\\locale\\"
            ]
        }
    ]
}